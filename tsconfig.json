{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "moduleResolution": "node",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "noEmit": true,
      "lib": ["dom", "esnext"],
      "allowJs": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }
  